import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
         int n;
         Scanner scan = new Scanner(System.in);
         int N1 = scan.nextInt();
         Scanner scan = new Scanner(System.in);
         int N2 = scan.nextInt();
         Scanner scan = new Scanner(System.in);
         int N3 = scan.nextInt();
         System.out.println(Max(N1,N2,N3), Min(N1,N2,N3), ((N1,N2,N3)/3));
    }
}
